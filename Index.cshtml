@model HomeInsexViewModel


@{
    ViewData["Title"] = "Home Page";
}
@await Html.PartialAsync("_Carousel", Model.carousels)

<h2 class="text text-center ">Laptops List </h2>
<div class="container marketing">
    <div class="row">
        @foreach (var item in Model.laptops)
        {
        <div class="col-lg-4">
            <img src="~/Images/@item.Image" class="img-fluid" alt="@item.Name image" />
            <h3>@item.Name</h3>
            <p><a class="btn btn-secondary" href="/home/details/@item.Id " role="button">View details >> </a></p>
        </div>

        }
    </div>
</div>